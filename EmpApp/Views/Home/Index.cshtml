@using DAL
@using BOL

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">List of Employes</h1>
    @{
        List<Employee> emps = DBManager.GetAllEmployees();
        <ol>
            @foreach (Employee emp in emps)
            {
                <li><a href="/home/details/@emp.Id">@emp.Name</a></li>
            }
        </ol>
    }

</div>
